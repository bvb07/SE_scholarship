select * from app_studentfile A inner join app_studentfilefile B on A.studentinform_profile_stdfile = B.file_id_id;
select * from app_studentfile A , app_studentfilefile B , app_studentinform C WHERE A.studentinform_profile_stdfile = B.file_id_id and A.stdinform_id_id = C.stdInform_id_id;
